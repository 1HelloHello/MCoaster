{
	"schemaVersion": 1,
	"id": "mcoaster",
	"version": "${version}",
	"name": "MCoaster",
	"description": "Adds smooth Roller Coasters to Minecraft. Fork of Splinecart",
	"authors": [
		"HelloHello"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/mcoaster",
		"sources": "https://github.com/1HelloHello/MCoaster-1.21.3"
	},
	"license": "MIT",
	"icon": "assets/splinecart/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"io.github.foundationgames.splinecart.Splinecart"
		],
		"client": [
			"io.github.foundationgames.splinecart.SplinecartClient"
		]
	},
	"mixins": [
		"splinecart.mixins.json",
		{
			"config": "splinecart.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*"
	}
}